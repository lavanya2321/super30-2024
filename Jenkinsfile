pipeline {
    agent any

    stages {
        stage('Git checkout') {
            steps {
                echo 'chekout GitHub repo'
            }
        }
        stage('Build') {
            steps {
                echo 'This is build stage'
            }
        }
        stage('Test') {
            steps {
                echo 'This is test stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is deploy stage'
            }
        }
    }
}
